{{! Any partials will go here }}
<article>
  <h2>{{title}}</h2>
  <p>Author: {{author_name}}</p>
  {{! cover ID }}

  <img src="https://covers.openlibrary.org/b/olid/{{cover_edition_key}}-M.jpg" alt="cover for {{title}}">
  {{! save/like/favorite button }}
  <p class="hidden">{{isbn}}</p>
  <a href="/api/book-page/{{isbn}}" id="book-results">Learn more</a>

  <button type='submit' class='liked'>Liked</button>
</article>

<script src='/js/savedBooks.js'></script>